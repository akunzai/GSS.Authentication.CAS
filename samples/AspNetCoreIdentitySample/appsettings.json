{
  "Authentication": {
    "CAS": {
      "ProtocolVersion": 3,
      "ServerUrlBase": "https://example.com/cas"
    },
    "OAuth": {
      "ClientId": "CHANGE_ME",
      "ClientSecret": "CHANGE_ME",
      "AuthorizationEndpoint": "https://example.com/oauth2/authorize",
      "TokenEndpoint": "https://example.com/oauth2/accessToken",
      "UserInformationEndpoint": "https://example.com/oauth2/profile"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "DataSource=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
